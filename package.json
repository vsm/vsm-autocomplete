{
  "name": "vsm-autocomplete",
  "version": "0.1.0",
  "description": "Vue-component for ontology lookup based on a vsm-dictionary",
  "main": "src/VsmAutocomplete.vue",
  "scripts": {
    "dev": "npm-run-all --parallel start testwi",
    "start": "webpack-dev-server --config webpack.config.js --env.NODE_ENV=development",
    "test": "mocha-webpack --require src/test-setup.js \"src/**/*.test.js\" --check-leaks --webpack-env.NODE_ENV=testing",
    "testw": "mocha-webpack --require src/test-setup.js \"src/**/*.test.js\" --check-leaks --webpack-env.NODE_ENV=testing --watch",
    "testwi": "mocha-webpack --require src/test-setup.js \"src/**/*.test.js\" --check-leaks --webpack-env.NODE_ENV=testing --watch --interactive=false",
    "lint": "eslint --ext .js,.vue src",
    "lintfix": "eslint --ext .js,.vue src --fix"
  },
  "keywords": [
    "autocomplete",
    "VSM",
    "dictionary",
    "ontology",
    "identifier",
    "term"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsmjs/vsm-autocomplete"
  },
  "author": "Steven Vercruysse (http://stcruy.com)",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/vsmjs/vsm-autocomplete/issues"
  },
  "homepage": "https://github.com/vsmjs/vsm-autocomplete#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@vue/test-utils": "^1.0.0-beta.14",
    "babel-loader": "^8.0.0-beta.2",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "document-register-element": "^1.8.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.5.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.8.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "npm-run-all": "^4.1.2",
    "null-loader": "^0.1.1",
    "opn-cli": "^3.1.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "text-transform-loader": "^2.0.0",
    "vsm-dictionary-cacher": "^1.0.7",
    "vsm-dictionary-local": "^1.4.0",
    "vue": "^2.5.16",
    "vue-custom-element": "^3.0.4",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "string-style-html": "^1.0.1"
  }
}
