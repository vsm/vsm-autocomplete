{
  "name": "vsm-autocomplete",
  "version": "1.1.0",
  "description": "Vue-component for ontology lookup based on a vsm-dictionary",
  "main": "src/VsmAutocomplete.vue",
  "scripts": {
    "dev": "npm-run-all --parallel start testwi",
    "start": "webpack-dev-server --config webpack.config.js --env.NODE_ENV=development",
    "test": "mocha-webpack --require src/test-setup.js \"src/**/*.test.js\" --check-leaks --webpack-env.NODE_ENV=testing",
    "testw": "mocha-webpack --require src/test-setup.js \"src/**/*.test.js\" --check-leaks --webpack-env.NODE_ENV=testing --watch",
    "testwi": "mocha-webpack --require src/test-setup.js \"src/**/*.test.js\" --check-leaks --webpack-env.NODE_ENV=testing --watch --interactive=false",
    "lint": "eslint --ext .js,.vue src"
  },
  "keywords": [
    "autocomplete",
    "VSM",
    "dictionary",
    "ontology",
    "vocabulary",
    "identifier",
    "term"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsmjs/vsm-autocomplete"
  },
  "author": "Steven Vercruysse (http://stcruy.com)",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/vsmjs/vsm-autocomplete/issues"
  },
  "homepage": "https://github.com/vsmjs/vsm-autocomplete#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "document-register-element": "^1.11.1",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "npm-run-all": "^4.1.3",
    "null-loader": "^0.1.1",
    "opn-cli": "^3.1.0",
    "sinon": "^6.2.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "text-transform-loader": "^2.0.0",
    "vsm-dictionary-cacher": "^1.1.1",
    "vsm-dictionary-local": "^2.3.2",
    "vue": "^2.5.17",
    "vue-custom-element": "^3.2.5",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "string-style-html": "^1.1.3"
  }
}
